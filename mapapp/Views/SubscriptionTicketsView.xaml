<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
			 x:Class="mapapp.Views.SubscriptionTicketsView">
	<ContentView.Content>
		<StackLayout VerticalOptions="Start" Margin="20">
			<Label Text="{Binding Title}" TextColor="#529eb6" />
			<Label Text="{Binding Description}" TextColor="#529eb6"/>
		</StackLayout>
	</ContentView.Content>
</ContentView>
